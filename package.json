{
  "name": "@attivio/suit",
  "version": "0.0.63",
  "description": "Attivio SUIT, the Search UI Toolkit, is a library for creating search clients for searching the Attivio platform.",
  "keywords": [
    "Attivio",
    "AIE",
    "SUIT",
    "Search UI",
    "Search",
    "React",
    "react-component"
  ],
  "homepage": "http://www.attivio.com",
  "bugs": {
    "url": "https://github.com/attivio/suit/issues",
    "email": "suitbugs@attivio.com"
  },
  "license": "SEE LICENSE IN LICENSE",
  "author": "Attivio, Inc.",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "repository": "https://github.com/attivio/suit",
  "scripts": {
    "build": "nwb build-react-component --copy-files",
    "clean": "nwb clean-module",
    "clean-node_modules": "run-script-os",
    "clean-node_modules:linux:darwin": "rm -rf ./node_modules",
    "clean-node_modules:win32": "rimraf ./node_modules",
    "doc": "run-script-os",
    "doc:win32": "rimraf docs/api && rimraf docs/util && documentation build src/api --shallow -f html -o docs/api && documentation build src/util --shallow -f html -o docs/util",
    "doc:linux:darwin": "rm -rf docs/api && rm -rf docs/util && documentation build src/api --shallow -f html -o docs/api && documentation build src/util --shallow -f html -o docs/util",
    "doclint": "documentation lint --shallow src/api && documentation lint --shallow src/util",
    "flow-kill": "run-script-os",
    "flow-kill:linux:darwin": "killall flow",
    "flow-kill:win32": "flow stop",
    "flow": "run-script-os",
    "flow:linux:darwin": "flow --show-all-errors --one-line src | grep -v node_modules | ./scripts/cleanflow.sh",
    "flow:win32": "flow --show-all-errors",
    "fullbuild": "run-script-os",
    "fullbuild:linux:darwin": "./scripts/fullbuild.sh",
    "fullbuild:win32": "./scripts/fullbuildWin.sh",
    "lint": "eslint src --ext .js --ext .jsx",
    "publishlib": "./scripts/publishlib.sh",
    "styleguide": "styleguidist build",
    "styleguide:serve": "styleguidist server",
    "test": "nwb test-react",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server",
    "updateversion": "./scripts/updateversion.sh",
    "validatecomponents": "./scripts/validatecomponents.sh",
    "watch": "run-script-os",
    "watch:linux:darwin": "nodemon --watch src --exec \" rm -rf .babelrc && npm run build\"",
    "watch:win32": "nodemon --watch src --exec \" rimraf .babelrc && npm run build\""
  },
  "dependencies": {
    "@mapbox/extent": "0.4.0",
    "@mapbox/mapbox-gl-draw": "1.0.4",
    "axios": "0.17.1",
    "crypto-js": "3.1.9-1",
    "enumify": "1.0.4",
    "extract-text-webpack-plugin": "3.0.0",
    "glamor": "2.20.40",
    "hygen": "1.5.8",
    "lodash": "^4.17.11",
    "mapbox-gl": "0.40.1",
    "moment": "2.20.1",
    "prop-types": "15.6.0",
    "query-string": "5.1.0",
    "react": "15.6.2",
    "react-addons-shallow-compare": "0.14",
    "react-bootstrap": "0.32.0",
    "react-bootstrap-table": "4.3.1",
    "react-dates": "12.7.1",
    "react-dom": "15.6.2",
    "react-highcharts": "13.0.0",
    "react-mapbox-gl": "2.5.0",
    "react-mapbox-gl-draw": "1.0.3",
    "react-overlays": "0.8.3",
    "react-router": "4.1.1",
    "react-router-dom": "4.1.1",
    "react-sizeme": "2.3.4",
    "react-toastify": "4.4.3",
    "validator": "8.2.0",
    "vis": "4.20.1",
    "webpack-merge": "4.1.2"
  },
  "devDependencies": {
    "babel-eslint": "7.2.3",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "create-nwb-webpack-config": "1.0.0",
    "css-loader": "1.0.1",
    "documentation": "8.1.2",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.1.0",
    "eslint-plugin-flowtype": "2.35.1",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.2.0",
    "flow-bin": "0.50.0",
    "inferno": "3.10.1",
    "karma-coverage-istanbul-instrumenter": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "less": "2.7.2",
    "less-loader": "4.0.5",
    "mocha": "^6.0.2",
    "nodemon": "1.15.1",
    "nwb": "0.21.x",
    "nyc": "^13.3.0",
    "react-styleguidist": "7.3.6",
    "rimraf": "latest",
    "run-script-os": "1.0.3",
    "style-loader": "0.18.2"
  }
}
